# When you are finished setting up your environment variables, rename this file to ".env" (without quotes).
# Be VERY sure that you do not commit your real environment variables!
# Also, all AUTH environment variables should be included in Vercel's configuration.

# use `npx auth secret`
AUTH_SECRET="{FILL THIS}"

AUTH_TRUST_HOST="true"

# Get these from their respective providers. At least one provider should be set up for login.
# To remove login providers, modify src/auth.ts
AUTH_GITHUB_ID="{FILL THIS}"
AUTH_GITHUB_SECRET="{FILL THIS}"
AUTH_GOOGLE_ID="{FILL THIS}"
AUTH_GOOGLE_SECRET="{FILL THIS}"

# Postgres environment variables
POSTGRES_DATABASE="verceldb"

POSTGRES_HOST="{FILL THIS}"
POSTGRES_PASSWORD="{FILL THIS}"
POSTGRES_PRISMA_URL="{FILL THIS}"
POSTGRES_URL="{FILL THIS}"
POSTGRES_URL_NON_POOLING="{FILL THIS}"
POSTGRES_URL_NO_SSL="{FILL THIS}"
POSTGRES_USER="default"